          </items>
        </Menu>
            <Menu mnemonicParsing="false" text="Navigate">
              <items>
                <MenuItem mnemonicParsing="false" text="Home" />
            <Menu mnemonicParsing="false" text="Album" />
              </items>
            </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" style="-fx-background-color: darkslategray;" VBox.vgrow="ALWAYS" />
  </children>
